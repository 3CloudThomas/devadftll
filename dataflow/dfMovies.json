{
	"name": "dfMovies",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "gen2MoviesCSV",
						"type": "DatasetReference"
					},
					"name": "MoviesCsv"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "DataLakeStorage",
						"type": "LinkedServiceReference"
					},
					"name": "deltaTable"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          movies as string,",
				"          Title as string,",
				"          genresgenregenre as string,",
				"          YEAR as string,",
				"          Rating as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> MoviesCsv",
				"MoviesCsv sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'delta',",
				"     fileSystem: 'deltalake',",
				"     mergeSchema: false,",
				"     autoCompact: false,",
				"     optimizedWrite: false,",
				"     vacuum: 0,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> deltaTable"
			]
		}
	}
}